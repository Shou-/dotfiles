"3.7.1 (created: 2013/05/22 20:32:57)

hi Hint font-family: "Chunkfive"; font-size: 12pt; color: #F90; text-shadow: 0 0 1px black, 0 0 1px black, 0 0 1px black, 0 0 1px black, 0 0 1px black;
hi HintActive background: #1f1f1f; color: #66ccff;
hi HintElem text-shadow: 0 0 2px white; color: #f6e9e9;
hi CmdLine background: url("http://a.pomf.se/fwyypa.png") fixed; color: #1d1d1d;
hi CmdLine>* font-family: "Asap"; font-size: 10pt;
hi Normal background: url("http://a.pomf.se/mdlxyp.png") fixed; color: #1d1d1d;
hi StatusLine color: #1d1d1d; font-family: "Asap"; font-size: 10pt;
hi CompItem background: url("http://a.pomf.se/fwyypa.png") fixed; text-shadow: 0 0 1px white; font-family: "Junction"; font-size: 10pt;
hi CompItem[selected] background: url("http://a.pomf.se/mdlxyp.png") fixed;
hi CompTitle background: url("http://a.pomf.se/mdlxyp.png") fixed;
hi CompTitle>* font-family: "Asap"; font-weight: bold; font-size: 12pt; padding-left: 5px; text-shadow: 0 0 1px white, 0 0 1px white; color: #2e2921;
hi CompDesc color: #1d1d1d; text-shadow: 0 0 1px white; max-width: 40vw; width: 40vw; overflow: hidden;
hi CompResult width: 40vw; max-width: 40vw; overflow: hidden; text-overflow: ellipsis;
hi ErrorMsg background-color: #170505; color: #e0e0e0 !important; text-shadow: 0 0 1px black; font-family: "Junction"; font-size: 10pt;
hi URL color: #1d1d1d; text-shadow: 0 0 1px white;
hi Filter font-family: "Asap"; font-size: 12pt; color: #F90; text-shadow: 0 0 1px black, 0 0 1px black, 0 0 1px black, 0 0 1px black, 0 0 1px black;
hi Indicator font-family: "Asap"; font-size: 12pt; color: #1d0a06; text-shadow: 0 0 1px white, 0 0 1px white;
hi InfoMsg color: #000942;
hi ModeMsg font-family: "Asap"; font-size: 12pt; color: #39C; text-shadow: 0 0 1px white, 0 0 1px white, 0 0 1px white, 0 0 1px white, 0 0 1px white; margin-left: 5px;

map V <Insert><C-a><Insert>
noremap j 3j
noremap k 3k
noremap <C-t> :tabgroups
set complete=slhbf
set defsearch=duckduckgo
set "editor=urxvt -e vim"
set hintchars=asdfghjkl:
